<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    

    <Color x:Key="DarkGreen" A="#FF" R="#3F" G="#4D" B="#40"/>
    <Color x:Key="MetalGrey" A="#FF" R="#BF" G="#BF" B="#BB"/>
    <Color x:Key="BackgroundGrey" A="#FF" R="#7B" G="#81" B="#79"></Color>
    <Color x:Key="ButtonGrey" A="#FF" R="#69" G="#6D" B="#66"/>
    <Color x:Key="ButtonOrange" A="#FF" R="#AA" G="#67" B="#46"/>
    <Color x:Key="ButtonDarkOrange" A="#FF" R="#92" G="#5E" B="#47"/>
    <Color x:Key="SliderOrange" A="#FF" R="#AB" G="#6A" B="#40"/>
    <Color x:Key="SliderDarkOrange" A="#FF" R="#96" G="#56" B="#2C"/>

    <SolidColorBrush x:Key="MainBackground" Color="{StaticResource DarkGreen}"/>

    <SolidColorBrush x:Key="OverlayBackground" Color="{StaticResource BackgroundGrey}"/>


    <SolidColorBrush x:Key="MainForeground" Color="{StaticResource MetalGrey}"/>


    <SolidColorBrush x:Key="ButtonBrush" Color="{StaticResource ButtonGrey}"/>

    <SolidColorBrush x:Key="ButtonHoverBrush" Color="{StaticResource ButtonOrange}"/>


</ResourceDictionary>